{
  "name": "docs-searchbar.js",
  "version": "1.1.6",
  "description": "Add a relevant search bar to your documentation using MeiliSearch",
  "keywords": [
    "documentation",
    "searchbar",
    "meilisearch",
    "search-as-you-type"
  ],
  "main": "dist/npm/index.js",
  "scripts": {
    "build": "./scripts/build",
    "build:css": "./scripts/build-css",
    "build:js": "./scripts/build-js",
    "doctoc": "doctoc --maxlevel 3 README.md",
    "format:scss": "prettier --write ./src/**/*.scss",
    "format:md": "prettier --write '**/*.md'",
    "lint": "eslint ./src ./*.js",
    "lint-fix": "eslint --fix ./src ./*.js",
    "prettier-ci": "prettier --check ./src/**/*.scss '**/*.md'",
    "serve": "./scripts/serve",
    "test": "./scripts/test",
    "test:watch": "./scripts/test-watch"
  },
  "files": [
    "dist/"
  ],
  "resolutions": {
    "braces": "^2.3.2",
    "node-sass": "^4.13.1",
    "mem": "^4.0.0"
  },
  "author": "MeiliSearch <bonjour@meilisearch.com> (https://github.com/meilisearch/)",
  "license": "MIT",
  "repository": "meilisearch/docs-searchbar.js",
  "devDependencies": {
    "autoprefixer": "9.8.4",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "10.1.0",
    "babel-istanbul": "0.12.2",
    "babel-jest": "23.6.0",
    "babel-loader": "7.1.5",
    "babel-plugin-rewire": "1.2.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-stage-3": "6.24.1",
    "cssnano": "4.1.10",
    "eslint": "^7.3.0",
    "eslint-config-algolia": "16.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "23.18.0",
    "eslint-plugin-prettier": "3.1.4",
    "jest": "23.6.0",
    "jsdom": "16.3.0",
    "json": "9.0.6",
    "live-server": "1.2.1",
    "mversion": "2.0.0",
    "node-sass": "4.14.1",
    "onchange": "7.0.2",
    "postcss-cli": "7.1.1",
    "prettier": "^2.0.5",
    "pretty-bytes-cli": "2.0.0",
    "semver": "7.3.2",
    "sinon": "9.0.2",
    "uglify-js": "3.10.0",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12"
  },
  "peerDependencies": {},
  "dependencies": {
    "autocomplete.js": "0.37.1",
    "concurrently": "^5.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "hogan.js": "^3.0.2",
    "meilisearch": "^0.11.0",
    "request": "^2.87.0",
    "stack-utils": "^2.0.2",
    "to-factory": "^1.0.0",
    "zepto": "^1.2.0"
  }
}
