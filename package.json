{
  "name": "docs-searchbar.js",
  "version": "1.3.3",
  "description": "Add a relevant search bar to your documentation using MeiliSearch",
  "keywords": [
    "documentation",
    "searchbar",
    "meilisearch",
    "search-as-you-type"
  ],
  "main": "dist/npm/index.js",
  "scripts": {
    "build": "./scripts/build",
    "build:css": "./scripts/build-css",
    "build:js": "./scripts/build-js",
    "format:scss": "prettier --write ./src/**/*.scss",
    "format:md": "prettier --write '**/*.md'",
    "playground": "yarn --cwd ./playgrounds/html && yarn --cwd ./playgrounds/html serve",
    "playground:build": "yarn --cwd ./playgrounds/html && yarn --cwd ./playgrounds/html build",
    "playground:js": "yarn --cwd ./playgrounds/javascript && yarn --cwd ./playgrounds/javascript serve",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "./scripts/test",
    "test:watch": "./scripts/test-watch",
    "test:e2e:watch": "concurrently --kill-others -s first \"yarn playground\" \"cypress open\"",
    "test:e2e": "concurrently --kill-others -s first \"yarn playground\" \"cypress run\""
  },
  "files": [
    "dist/"
  ],
  "author": "MeiliSearch <bonjour@meilisearch.com> (https://github.com/meilisearch/)",
  "license": "MIT",
  "repository": "meilisearch/docs-searchbar.js",
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/register": "^7.15.3",
    "autoprefixer": "^9.8.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.2",
    "babel-loader": "^8.2.1",
    "babel-plugin-rewire": "^1.2.0",
    "concurrently": "^6.3.0",
    "cssnano": "^4.1.11",
    "cypress": "^8.5.0",
    "eslint": "^7.21.0",
    "eslint-config-algolia": "^16.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.1.8",
    "eslint-plugin-prettier": "^3.3.0",
    "jest": "^26.6.3",
    "jsdom": "^17.0.0",
    "node-sass": "^6.0.1",
    "postcss-cli": "^7.1.2",
    "prettier": "^2.0.5",
    "pretty-bytes-cli": "^2.0.0",
    "sinon": "^11.1.2",
    "uglify-js": "^3.14.2",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.9.0"
  },
  "dependencies": {
    "autocomplete.js": "^0.38.0",
    "hogan.js": "^3.0.2",
    "meilisearch": "^0.20.1",
    "to-factory": "^1.0.0",
    "zepto": "^1.2.0"
  }
}
