{
  "name": "docs-searchbar.js",
  "version": "1.1.11",
  "description": "Add a relevant search bar to your documentation using MeiliSearch",
  "keywords": [
    "documentation",
    "searchbar",
    "meilisearch",
    "search-as-you-type"
  ],
  "main": "dist/npm/index.js",
  "scripts": {
    "build": "./scripts/build",
    "build:css": "./scripts/build-css",
    "build:js": "./scripts/build-js",
    "doctoc": "doctoc --maxlevel 3 README.md",
    "format:scss": "prettier --write ./src/**/*.scss",
    "format:md": "prettier --write '**/*.md'",
    "lint": "eslint ./src ./*.js",
    "lint-fix": "eslint --fix ./src ./*.js",
    "prettier-ci": "prettier --check ./src/**/*.scss '**/*.md'",
    "serve": "./scripts/serve",
    "test": "./scripts/test",
    "test:watch": "./scripts/test-watch"
  },
  "files": [
    "dist/"
  ],
  "resolutions": {
    "braces": "^2.3.2",
    "node-sass": "^4.13.1",
    "mem": "^4.0.0"
  },
  "author": "MeiliSearch <bonjour@meilisearch.com> (https://github.com/meilisearch/)",
  "license": "MIT",
  "repository": "meilisearch/docs-searchbar.js",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "autoprefixer": "^9.8.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.1",
    "babel-plugin-rewire": "^1.2.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.3.0",
    "eslint-config-algolia": "^16.0.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "jest": "^26.6.3",
    "jsdom": "^16.4.0",
    "live-server": "^1.2.1",
    "mversion": "^2.0.0",
    "node-sass": "^5.0.0",
    "onchange": "^7.1.0",
    "postcss-cli": "^7.1.2",
    "prettier": "^2.0.5",
    "pretty-bytes-cli": "^2.0.0",
    "semver": "^7.3.4",
    "sinon": "^9.2.0",
    "uglify-js": "^3.12.0",
    "webpack": "^5.13.0",
    "webpack-cli": "^4.3.1"
  },
  "peerDependencies": {},
  "dependencies": {
    "autocomplete.js": "^0.38.0",
    "concurrently": "^5.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "hogan.js": "^3.0.2",
    "meilisearch": "^0.17.0",
    "request": "^2.87.0",
    "stack-utils": "^2.0.2",
    "to-factory": "^1.0.0",
    "zepto": "^1.2.0"
  }
}
